<!-- Ghost Kit CXform Module -->
<ServerManagerConfiguration>
<!-- Begin SpaceCraftInfo -->
<ProxyGroup name="filters">

    <SourceProxy class="gk_cxFormField"
                 label="GK Field Transform"
                 name="GKFieldTransform">
      <Documentation long_help="This filter provides coordinate transform abuility on Field Data"
                     short_help="provides coordinate transform abuility on Field Data">

      </Documentation>

      <InputProperty clean_command="RemoveAllInputs"
                     command="AddInputConnection"
                     multiple_input="1"
                     name="Input">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>

        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet" />
          <DataType value="vtkTable" />
          <DataType value="vtkCompositeDataSet" />
        </DataTypeDomain>

        <Documentation>Set the input of the filter.</Documentation>

      </InputProperty>

<!--Transform Selectors-->
    <IntVectorProperty name="Source"
       label="From:"
       command="SetSourceSystem"
       number_of_elements="1"
       default_values="12"
       panel_visibility="default">

          <EnumerationDomain name="enum">
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

    <IntVectorProperty name="Destination"
            label="To:"
            command="SetDestSystem"
            number_of_elements="1"
            default_values="6"
            panel_visibility="default">

           <EnumerationDomain name="enum">
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />

          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>


<!--Field Selections-->
      <StringVectorProperty
            name="FieldArrayInfo"
            information_only="1">
            <ArraySelectionInformationHelper attribute_name="Field"/>

            <Documentation>
                This is the Field Selector.
            </Documentation>

      </StringVectorProperty>

      <StringVectorProperty
            name="FieldArrayStatus"
            label="Avaiable Vector Fields"
            command="SetFieldArrayStatus"
            number_of_elements="0"
            repeat_command="1"
            number_of_elements_per_command="2"
            element_types = "2 0"
            information_property="FieldArrayInfo"
            default_values = "0">

            <ArraySelectionDomain name="array_list">
                <RequiredProperties>
                    <Property name="FieldArrayInfo" function="ArrayList"/>
                </RequiredProperties>
            </ArraySelectionDomain>

            <Documentation>
                This is the Fileds Selector.
            </Documentation>

      </StringVectorProperty>


<!--Manual Selections-->
    <IntVectorProperty name="useManual"
                   label="Use Manual Transform"
                   command="setUseManual"
                   number_of_elements="1"
                   default_values="1"
                   panel_visibility="advanced">
        <BooleanDomain name="bool"/>
    </IntVectorProperty>

      <StringVectorProperty command="SetManualName"
                            label="Output Name:"
                            name="ManualFieldName"
                            number_of_elements="1"
                            panel_visibility="advanced">
        <Documentation>
        </Documentation>
      </StringVectorProperty>

    <DoubleVectorProperty name="ManualInput"
            label="Manual Input:"
            command="SetManualInput"
            number_of_elements="3"
            default_values="0 0 0"
            panel_visibility="advanced">

    </DoubleVectorProperty>

    <DoubleVectorProperty name="ManualOutput"
            label="Manual Output:"
            command="SetManualOutput"
            number_of_elements="3"
            default_values="0 0 0"
            panel_visibility="never">

    </DoubleVectorProperty>

    <IntVectorProperty name="manualFrom"
       label="From:"
       command="SetManualFromSystem"
       number_of_elements="1"
       default_values="0"
       panel_visibility="never">

          <EnumerationDomain name="enum">
                <Entry value="0" text="USE MASTER" />
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

    <IntVectorProperty name="manualTo"
       label="To:"
       command="SetManualToSystem"
       number_of_elements="1"
       default_values="0"
       panel_visibility="never">

          <EnumerationDomain name="enum">
                <Entry value="0" text="USE MASTER" />
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

<!--Split Field Selection-->
      <IntVectorProperty name="useSplit"
                   label="Use Split Transform"
                   command="setUseSplit"
                   number_of_elements="1"
                   default_values="1"
                   panel_visibility="advanced">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>

      <StringVectorProperty command="SetSplitFieldName"
                            label="Output Name:"
                            name="SplitFieldName"
                            number_of_elements="1"
                            panel_visibility="advanced">
        <Documentation>
        </Documentation>
      </StringVectorProperty>

      <IntVectorProperty
         label="X Field:"
         name="SplitFieldX"
         command="SetSplitX"
         number_of_elements="1"
         default_values="0"
         panel_visibility="advanced">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Entry 0"/>
          <Entry value="1" text="Entry 1"/>
          <Entry value="2" text="Entry 2"/>
          <Entry value="3" text="Entry 3"/>
        </EnumerationDomain>
        <Documentation>
            This is a placeholder for the Split Field Transform
        </Documentation>
      </IntVectorProperty>

      <IntVectorProperty
         label="Y Field:"
         name="SplitFieldY"
         command="SetSplitY"
         number_of_elements="1"
         default_values="0"
         panel_visibility="advanced">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Entry 0"/>
          <Entry value="1" text="Entry 1"/>
          <Entry value="2" text="Entry 2"/>
          <Entry value="3" text="Entry 3"/>
        </EnumerationDomain>
        <Documentation>
            This is a placeholder for the Split Field Transform
        </Documentation>
      </IntVectorProperty>

      <IntVectorProperty
         label="Z Field:"
         name="SplitFieldZ"
         command="SetSplitZ"
         number_of_elements="1"
         default_values="0"
         panel_visibility="advanced">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Entry 0"/>
          <Entry value="1" text="Entry 1"/>
          <Entry value="2" text="Entry 2"/>
          <Entry value="3" text="Entry 3"/>
        </EnumerationDomain>
        <Documentation>
            This is a placeholder for the Split Field Transform
        </Documentation>
      </IntVectorProperty>


    <IntVectorProperty name="splitFrom"
       label="From:"
       command="SetSplitFromSystem"
       number_of_elements="1"
       default_values="0"
       panel_visibility="never">

          <EnumerationDomain name="enum">
                <Entry value="0" text="USE MASTER" />
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

    <IntVectorProperty name="splitTo"
       label="To:"
       command="SetSplitToSystem"
       number_of_elements="1"
       default_values="0"
       panel_visibility="never">

          <EnumerationDomain name="enum">
                <Entry value="0" text="USE MASTER" />
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

      <!-- Define property groups for better readability of panels -->
      <PropertyGroup label="Master Transform Operation">
        <Property name="Source" />
        <Property name="Destination" />
      </PropertyGroup>

      <PropertyGroup label="Vector Field Transform">
        <Property name="FieldArrayStatus" />
      </PropertyGroup>

      <PropertyGroup label="Manual Field Transform">
        <Property name="useManual" />
        <Property name="ManualFieldName" />
        <Property name="ManualInput" />
        <Property name="ManualOutput" />
        <Property name="manualFrom" />
        <Property name="manualTo" />

      </PropertyGroup>

      <PropertyGroup label="Split Field Transform">
        <Property name="useSplit" />
        <Property name="SplitFieldName" />
        <Property name="SplitFieldX" />
        <Property name="SplitFieldY" />
        <Property name="SplitFieldZ" />
        <Property name="splitFrom" />
        <Property name="splitTo" />

      </PropertyGroup>

      <Hints>
        <Visibility replace_input="0" />
        <OutputPort index="0"
                    name="Output-0"
                    type="text" />
      </Hints>
      <!-- End PythonAnnotation -->
    </SourceProxy>

</ProxyGroup>

<!-- End SpaceCraftInfo -->
</ServerManagerConfiguration>
