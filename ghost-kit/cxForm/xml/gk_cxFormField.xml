<!-- Ghost Kit CXform Module -->
<ServerManagerConfiguration>
<!-- Begin SpaceCraftInfo -->
<ProxyGroup name="filters">

    <SourceProxy class="gk_cxFormField"
                 label="GK Field Transform"
                 name="GKFieldTransform">
      <Documentation long_help="This filter provides coordinate transform abuility on Field Data"
                     short_help="provides coordinate transform abuility on Field Data">

      </Documentation>

      <InputProperty clean_command="RemoveAllInputs"
                     command="AddInputConnection"
                     multiple_input="1"
                     name="Input">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>

        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet" />
        </DataTypeDomain>

        <Documentation>Set the input of the filter.</Documentation>

      </InputProperty>

    <IntVectorProperty name="SourceCoord"
       label="Source System"
       command="SetSourceSystem"
       number_of_elements="1"
       default_values="12"
       panel_visibility="default">

          <EnumerationDomain name="enum">
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />
          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>

    <IntVectorProperty name="DestCoord"
            label="Destination System"
            command="SetDestSystem"
            number_of_elements="1"
            default_values="6"
            panel_visibility="default">

           <EnumerationDomain name="enum">
                <Entry value="1" text="J2000"       />
                <Entry value="2" text="GEI"         />
                <Entry value="3" text="GEO"         />
                <Entry value="4" text="MAG"         />
                <Entry value="5" text="GSE"         />
                <Entry value="6" text="GSM"         />
                <Entry value="7" text="SM"          />
                <Entry value="8" text="RTN"         />
                <Entry value="9" text="GSEQ"        />
                <Entry value="10" text="HEE"        />
                <Entry value="11" text="HAE"        />
                <Entry value="12" text="HEEQ"       />
                <Entry value="13" text="HEEQ+180"   />

          </EnumerationDomain>

          <Documentation>

          </Documentation>

    </IntVectorProperty>


      <StringVectorProperty command="SetInputVariable"
                            name="Input Field Variable"
                            number_of_elements="1">
        <Documentation>
        </Documentation>
      </StringVectorProperty>


    <DoubleVectorProperty name="ManualInput"
            label="Manual Data Entry"
            command="SetManualSource"
            number_of_elements="3"
            default_values="0 0 0"
            panel_visibility="default">

    </DoubleVectorProperty>

    <DoubleVectorProperty name="OutputValue"
            label="Output Values"
            command="SetOutputValue"
            number_of_elements="3"
            default_values="0 0 0"
            panel_visibility="never">

    </DoubleVectorProperty>

      <Hints>
        <Visibility replace_input="0" />
        <OutputPort index="0"
                    name="Output-0"
                    type="text" />
      </Hints>
      <!-- End PythonAnnotation -->
    </SourceProxy>














