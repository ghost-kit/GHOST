<!-- Ghost Kit CXform Module -->
<ServerManagerConfiguration>
<!-- Begin SpaceCraftInfo -->
    <ProxyGroup name="filters">
    <SourceProxy name="Ghost Kit Coordiante Transform" class="gk_cxform">

       <InputProperty name="Input" command="SetInputConnection">
          <ProxyGroupDomain name="groups">
            <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkDataSet"/>
<!--            <DataType value="vtkUnstructuredGrid"/>-->
          </DataTypeDomain>

          <InputArrayDomain name="input_array" attribute_type="point"
                            number_of_components="1" optional="0"/>

       </InputProperty>


            <IntVectorProperty name="SourceCoord"
                                label="Source Coordinate System"
                                command="SetSourceSystem"
                                number_of_elements="1"
                                default_values="2"
                                panel_visibility="default">

                <EnumerationDomain name="enum">
                    <Entry value="0" text="TBA0"/>
                    <Entry value="1" text="TBA1"/>
                    <Entry value="2" text="TBA2"/>
                </EnumerationDomain>

                <Documentation>

                </Documentation>

            </IntVectorProperty>

            <IntVectorProperty name="DestCoord"
                                label="Destination Coordinate System"
                                command="SetDestSystem"
                                number_of_elements="1"
                                default_values="2"
                                panel_visibility="default">

                <EnumerationDomain name="enum">
                    <Entry value="0" text="TBA0"/>
                    <Entry value="1" text="TBA1"/>
                    <Entry value="2" text="TBA2"/>
                </EnumerationDomain>

                <Documentation>

                </Documentation>

            </IntVectorProperty>


    </SourceProxy>
    </ProxyGroup>
    <!-- End SpaceCraftInfo -->


</ServerManagerConfiguration>
